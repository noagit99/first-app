{
    "rewrites": [
      { "source": "/(.*)", "destination": "/index.html" }
    ],
    "cleanUrls": true,
    "outputDirectory": "dist",
    "trailingSlash": false,
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache"
          }
        ]
      }
    ]
  }